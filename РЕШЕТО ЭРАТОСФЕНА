#include<iostream>//для работы с консолью
using namespace std;//пространство имен стд

int main()
{
	//объявление переменных
	int n;//a-введенное число пользователем
	setlocale(0, "");//русификация
	cout << "Введите число: ";//ввод числа
	cin >> n;
	if (!cin)//проверка на дурака
	{
		cout << "Ошибка ввода: " << endl;
	}
	int* a = new int[n + 1];//динамический массив для записи отсортированных чисел
	for (int i = 0; i < n + 1; i++)//заполнение динмического массива
	{
		a[i] = i;
	}
	for (int p= 2; p < n + 1; p++)//пройдемся по массиву 
	{
		if (a[p] != 0)//если его элемент не равен 0
		{
			cout << a[p] << endl;//
			for (int j = p * p; j < n + 1; j += p)//удалить все числа из массива от квадрата числа с шагом числа(p^2,p^2+p,p^2+2p и тд.)
			{
				a[j] = 0;
			}
		}
	}
}
