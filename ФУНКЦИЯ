//Для произвольных x, y и b вычислить функцию данную.
#include<cmath>//библиотека для квадратного корня и натурального логарифма
#include <iostream>//библиотека для вывода в консоль
using namespace std;//простарнство имен стд

int main()
{
	setlocale(0, "");//русификация консоли
	double x, y, b;//переменные данные
	cout << "Введите переменную x" << endl;
	cin >> x;
	if (!cin) {//проверка на дурака
		cout << "Ввод неверен" << endl;
		return 0;
	}
	cout << "Введите переменную y" << endl;
	cin >> y;
	if (!cin) {//проверка на дурака
		cout << "Ввод неверен" << endl;
		return 0;
	}
	cout << "Введите переменную b" << endl;
	cin >> b;
	if (!cin) {//проверка на дурака
		cout << "Ввод неверен" << endl;
		return 0;
	}
	if (((b - x) >= 0) || ((b - y) > 0)) {//соблюдение ОДЗ и вычисление функции
		cout << "Результат данной функции z= " << log(b - y) * sqrt(b - x);
		return 0;
	}
	else {
		cout << "Результат невозможен" << endl;
		return 0;
	}
}
